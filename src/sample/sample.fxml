                                       <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                    </FlowPane.margin>
                                 </Label>
                                 <ComboBox fx:id="protocolComboBox" prefWidth="150.0" promptText="Select protocol...">
                                    <FlowPane.margin>
                                       <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                    </FlowPane.margin>
                                 </ComboBox>
                                 <Label layoutX="20.0" layoutY="24.0" text="IP Address:">
                                    <FlowPane.margin>
                                       <Insets bottom="10.0" left="40.0" right="10.0" top="10.0" />
                                    </FlowPane.margin>
                                 </Label>
                                 <TextField>
                                    <FlowPane.margin>
                                       <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                    </FlowPane.margin>
                                 </TextField>
                              </children>
                              <VBox.margin>
                                 <Insets />
                              </VBox.margin>
                           </FlowPane>
                        </children>
                     </VBox>
                  </children></AnchorPane>
         </content>
       </Tab>
     </tabs>
   </TabPane>
