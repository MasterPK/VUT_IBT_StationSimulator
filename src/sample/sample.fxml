                                 <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                              </FlowPane.margin>
                           </TextField>
                           <Button fx:id="sendButton" mnemonicParsing="false" onMouseClicked="#sendRequest" text="Send">
